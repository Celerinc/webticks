    "license": "MPL-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Celerinc/webticks.git",
        "directory": "packages/nuxt"
    },
    "homepage": "https://github.com/Celerinc/webticks#readme",
    "bugs": {
        "url": "https://github.com/Celerinc/webticks/issues"
    },
    "dependencies": {
        "@webticks/core": "^0.1.1"
    },
    "devDependencies": {
        "@nuxt/kit": "^3.0.0",
        "@nuxt/module-builder": "^0.8.0"
    }
